{
    "name": "phossa2/cache",
    "license": "MIT",
    "type": "library",
    "description": "A PSR-6 compliant PHP caching library",
    "keywords": ["cache", "caching", "psr-6", "phossa", "phossa2"],
    "homepage": "https://github.com/phossa2/cache",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa2"
        }
    ],
    "require": {
        "php": "~5.4.0|~7.0.0",
        "psr/cache": "^1.0",
        "phossa2/shared": "^2.0.27",
        "phossa2/event": "^2.1.6",
        "phossa2/storage": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": { "Phossa2\\Cache\\": "Cache/" }
    },
    "scripts": {
        "test": "phpunit",
        "check": "phpcs --standard=psr2 Cache/",
        "format": "phpcbf --standard=psr2 Cache/"
    },
    "provide": {
        "psr/cache-implementation": "1.0.0"
    },
    "suggest": {
        "psr/log-implementation": "Allows log messages"
    },
    "config": {
        "platform": {
            "php": "5.4.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "archive": {
        "exclude": [ "!vendor", "tests", "composer.lock" ]
    }
}
