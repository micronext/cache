{
    "name": "phossa2/cache",
    "license": "MIT",
    "type": "library",
    "description": "A PSR-6 compliant PHP caching library",
    "keywords": ["cache", "caching", "psr-6", "phossa"],
    "homepage": "https://github.com/phossa2/cache",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa2"
        }
    ],
    "require": {
        "php": "~5.4|~7.0",
        "psr/cache": "^1.0",
        "phossa2/storage": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-0": { "Phossa2\\Cache\\": "src/" }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "provide": {
        "psr/cache-implementation": "1.0.0"
    },
    "replace": {
        "phossa/phossa-cache": "*"
    },
    "suggest": {
        "psr/log-implementation": "Allows log messages"
    },
    "config": {
        "platform": {
            "php": "5.4.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "archive": {
        "exclude": [ "!vendor", "tests", "composer.lock" ]
    }
}
